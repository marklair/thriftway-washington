= stylesheet_link_tag    "detail_pages"

#downloads
  %section#feature-header
    .container
      %h1 Downloads

.container
  %section#features-list
    .row-fluid
      .span8
        %h2 Conference Slides


        %ul.feature-detail
          %li= link_to "Print to Digital at the 2013 NGA Show", image_path('downloads/print_to_digital_slides_NGA_conference_2013.pdf'), download: 'print_to_digital_slides_NGA_conference_2013'

        %hr

        %h2 Company Media

        %ul.feature-detail
          %li
            = image_tag 'downloads/webstop_logo_blue.png'
            %br
            = link_to "Small Webstop Logo (190px X 70px)" , image_path('downloads/webstop_logo_blue.png')

        %hr

      .span4
        .features-nav-section
          %h4.heading More Features To Explore
          = render 'grocery_digital_marketing/features_navigation'
